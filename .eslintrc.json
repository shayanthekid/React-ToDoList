{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        // "eslint:recommended",
        // "plugin:react/recommended"
        "react-app", "prettier"
    ],
    "parserOptions": {
        // "ecmaFeatures": {
        //     "jsx": true
        // },
        "ecmaVersion": 11
        // "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "printWidth": 80,
"trailingComma": "es5",
"semi": false,
"jsxSingleQuote": true,
"singleQuote": true,
"useTabs": true
            }
        ]
    }
}
